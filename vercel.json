{
    "version": 2,
    "builds": [{
        "src": "app.py",
        "use": "@vercel/python"
    }],
    "routes": [{
            "src": "/_vercel/speed-insights/script\\.js",
            "dest": "/_vercel/speed-insights/script.js"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "github": {
        "silent": true
    }
}