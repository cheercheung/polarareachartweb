{
    "version": 2,
    "builds": [{
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "10mb",
            "runtime": "python3.9"
        }
    }],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "app.py"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}